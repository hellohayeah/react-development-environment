{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "noEmit": true,
    "allowJs": true
  },
  "include": ["src"],
  "exclude": ["blueprint-templates"]
}
