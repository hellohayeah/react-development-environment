  "main": "index.js",
  "scripts": {
    "start": "rimraf dist && npm run dev:client && npm run dev:server && node ./dist/server/server",
    "dev:server": "cross-env TS_NODE_PROJECT=\"tsconfigr-webpack.json\" webpack --config config/server.dev.ts",
    "dev:client": "cross-env TS_NODE_PROJECT=\"tsconfigr-webpack.json\" webpack --config config/client.dev.ts",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cross-env TS_NODE_PROJECT=\"tsconfigr-webpack.json\" webpack --config config/server.prod.ts",
    "build:client": "cross-env TS_NODE_PROJECT=\"tsconfigr-webpack.json\" webpack --config config/client.prod.ts"
  },
